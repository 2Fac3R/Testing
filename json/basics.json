{
    "name": "John"
} // strings
{
    "age": 30
} // Numbers
{ // Objects
    "employee": {
        "name": "John",
        "age": 30,
        "city": "New York"
    }
} { //arrays
    "employees": [
        "John",
        "Anna",
        "Peter"
    ]
} {
    "sale": true
} // boolean
{
    "middlename": null
} // null
// Nested JSON Objects
myObj = {
    "name": "John",
    "age": 30,
    "cars": [
        "car1": "Ford",
        "car2": "BMW",
        "car3": "Fiat"
    ]
}